{
  "name": "XSS Hunter",
  "action": {},
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js":["jquery.min.js", "background.js"],
      "matches": ["*://*/*"]
    }
  ],
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  // "background": {
  //   "service_worker": "background.js"
  // },
  "web_accessible_resources": [{
    "resources": ["query.min.js"],
    "matches": ["*://*/*"]
  }]
  
}